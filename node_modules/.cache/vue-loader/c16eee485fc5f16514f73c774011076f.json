Agregar tutorial</b-navbar-brand>\n      </b-navbar>\n    </div>\n    <b-form @submit=\"onSubmit\" @reset=\"onReset\" class=\"mt-3\">\n      <b-form-group id=\"input-group-1\" label-for=\"input-1\">\n        <b-form-input\n          id=\"input-1\"\n          v-model=\"form.titulo\"\n          type=\"text\"\n          required\n          placeholder=\"TÃ­tulo\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group>\n        <b-form-input\n          id=\"input-2\"\n          v-model=\"form.profesor\"\n          required\n          placeholder=\"Profesor\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group>\n        <b-form-input\n          id=\"input-3\"\n          v-model=\"form.materia\"\n          required\n          placeholder=\"Materia\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-form-group>\n        <b-form-input\n          id=\"input-4\"\n          type=\"datetime-local\"\n          v-model=\"form.fecha\"\n          required\n          placeholder=\"Fecha\"\n        ></b-form-input>\n      </b-form-group>\n\n      <b-button type=\"submit\" variant=\"primary\">Agregar</b-button>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport axios from 'axios'\n\nexport default {\n  data() {\n    return {\n      form: {\n        titulo: \"\",\n        profesor: \"\",\n        materia: \"\",\n        fecha: \"\"\n      }\n    };\n  },\n  methods: {\n    onSubmit(evt) {\n      evt.preventDefault();\n      let agregando = {\n        nombre: this.form.titulo,\n        profesor: this.form.profesor,\n        materia: this.form.materia,\n        fecha: this.form.fecha\n      };\n      axios.post('https://rayentutorialtestapp.azurewebsites.net/createtutorial', agregando).then((res) => {\n        if(res.data.id !== undefined){\n          alert('Tutorial agregado correctamente')\n          this.$router.push({ name: \"Home\" });\n        }else{\n          alert('Error al ingresar tutorial')\n        }\n      })\n    },\n    onReset(evt) {\n      evt.preventDefault();\n      // Reset our form values\n    },\n  }\n};\n</script>\n\n<style lang=\"scss\" scoped>\n.tutorial {\n  background-color: #6f42c1;\n}\n</style>\n"]}]}